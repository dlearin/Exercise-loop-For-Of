<body>
    <script type="text/javascript">

        let number=[];
        let multiplication=0;

        for(let i=0; i<5; i++) {

            number[i]=parseInt(prompt("enter your number"));

        }

        document.write("The result of multiplying the numbers by 5 is: ")

            for (multiplication of number) {

                document.write(multiplication + "x 5 = " + multiplication*5 + "<br>");

            }



    </script>
